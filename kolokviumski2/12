#include <iostream>
using namespace std;

int BrojPozitivni(int a[], int n)
{
    int count = 0;
    for(int i = 0; i < n; i++)
    {
        if(a[i] > 0)
        {
            count++;
        }
    }
    return count;
}
int main()
{
    int a[100], n;
    cin>>n;
    for(int i = 0; i < n; i++)
    {
        cin>>a[i];
    }
    
    int result = BrojPozitivni(a, n);
    cout<<result<<endl;

}
