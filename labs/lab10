#include <iostream>
using namespace std;

int main()
{
    int a, b;
    cin>>a;
    cin>>b;
    cout<<"Vrednosta na broj1 i broj2 pred zamena na broevite e: "<<a<<","<<b<<" a razlikata: "<<a - b<<endl;
    cout<<"Vrednosta na broj1 i broj2 posle zamena na broevite e: "<<b<<","<<a<<" a razlikata: "<<b - a<<endl;
    return 0;
}
